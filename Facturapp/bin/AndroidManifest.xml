            android:label="@string/title_activity_managecatprod"
            android:theme="@style/Theme.AppCompat" >
        </activity>
        <activity
            android:name=".ClientesAct"
            android:label="@string/title_activity_clientes" >
        </activity>
        <activity
            android:name=".Notas"
            android:label="@string/title_activity_notas" >
        </activity>
        <activity
            android:name=".ProdsCat"
            android:label="@string/title_activity_prods_cat" >
        </activity>
    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

</manifest>
